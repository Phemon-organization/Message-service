      "dev-id-1",
      "dev-id-2"
    ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "15s",
        "Limit": 100
      }
    ]
  },
  "ConnectionStrings": {
    "Net6WebApiConnection": "Server=host.docker.internal,1434;Database=Phemon;Integrated Security=false;User ID=SA;Password=Admin123!;TrustServerCertificate=True;"
